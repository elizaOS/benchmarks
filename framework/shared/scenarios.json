      "description": "Messages without agent name — shouldRespond must evaluate via mock LLM",
      "messages": [
        { "content": "Hello there, how are you?", "role": "user" },
        { "content": "Tell me about performance testing.", "role": "user" },
        { "content": "What's the weather like?", "role": "user" },
        { "content": "Can you help me with something?", "role": "user" },
        { "content": "Thanks for the help.", "role": "user" }
      ],
      "config": {
        "checkShouldRespond": true,
        "multiStep": false,
        "warmup": 3,
        "iterations": 20
