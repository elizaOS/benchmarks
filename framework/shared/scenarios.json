{
  "scenarios": [
    {
      "id": "single-message",
      "name": "Single Message Processing",
      "description": "Process one message end-to-end (baseline latency)",
      "messages": [
        { "content": "Hello BenchmarkAgent, how are you?", "role": "user" }
      ],
      "config": {
        "checkShouldRespond": false,
        "multiStep": false,
        "warmup": 5,
        "iterations": 50
      }
    },
    {
      "id": "conversation-10",
      "name": "10-Message Conversation",
      "description": "10-message back-and-forth conversation measuring state growth",
      "messages": [
        { "content": "Hello BenchmarkAgent, let's talk.", "role": "user" },
        { "content": "BenchmarkAgent, what do you think about performance?", "role": "user" },
        { "content": "BenchmarkAgent, can you explain latency?", "role": "user" },
        { "content": "BenchmarkAgent, how about throughput?", "role": "user" },
        { "content": "BenchmarkAgent, what about memory usage?", "role": "user" },
        { "content": "BenchmarkAgent, tell me about concurrency.", "role": "user" },
        { "content": "BenchmarkAgent, how does state composition work?", "role": "user" },
        { "content": "BenchmarkAgent, what are providers?", "role": "user" },
        { "content": "BenchmarkAgent, explain action dispatch.", "role": "user" },
        { "content": "BenchmarkAgent, summarize everything.", "role": "user" }
      ],
      "config": {
        "checkShouldRespond": false,
        "multiStep": false,
        "warmup": 3,
        "iterations": 20
      }
    },
    {
      "id": "conversation-100",
      "name": "100-Message Conversation",
      "description": "100-message conversation measuring state growth impact at scale",
      "messages": "_generate:100",
      "config": {
        "checkShouldRespond": false,
        "multiStep": false,
        "warmup": 2,
        "iterations": 5
      }
    },
    {
      "id": "burst-100",
      "name": "100-Message Burst",
      "description": "100 messages sent as fast as possible (sequential throughput)",
      "messages": "_generate:100",
      "config": {
        "checkShouldRespond": false,
        "multiStep": false,
        "warmup": 2,
        "iterations": 5
      }
    },
    {
      "id": "burst-1000",
