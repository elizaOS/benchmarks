        { "content": "Hey BenchmarkAgent, respond to this.", "role": "user" },
        { "content": "BenchmarkAgent, another message.", "role": "user" },
        { "content": "BenchmarkAgent, third test.", "role": "user" },
        { "content": "BenchmarkAgent, fourth test.", "role": "user" },
        { "content": "BenchmarkAgent, fifth test.", "role": "user" }
