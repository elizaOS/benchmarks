{
  "name": "@elizaos/configbench",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Configuration & Secrets Security Benchmark for ElizaOS plugin-secrets-manager and plugin-plugin-manager",
  "main": "src/index.ts",
